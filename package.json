{
  "name": "@storm-software/storm-ops",
  "version": "0.0.1",
  "private": true,
  "description": "âš¡The Storm-Ops monorepo contains utility applications, tools, and various libraries to create modern and scalable web applications.",
  "keywords": [
    "storm",
    "storm-ops",
    "stormstack",
    "storm-stack",
    "acidic",
    "acidic-lang",
    "acidic-model",
    "impact",
    "nextjs",
    "prisma",
    "zenstack",
    "hasura",
    "strapi",
    "graphql",
    "sullivanpj",
    "open-system",
    "monorepo"
  ],
  "homepage": "https://stormsoftware.org",
  "bugs": {
    "url": "https://github.com/storm-software/storm-ops/issues",
    "email": "support@stormsoftware.org"
  },
  "repository": {
    "type": "github",
    "url": "https://github.com/storm-software/storm-ops.git"
  },
  "license": "Apache 2.0",
  "author": {
    "name": "Storm Software",
    "email": "contact@stormsoftware.org",
    "url": "https://stormsoftware.org"
  },
  "scripts": {
    "adr": "pnpm log4brains adr new",
    "adr-preview": "pnpm log4brains preview",
    "prebuild": "pnpm clean",
    "build": "nx affected -t build --parallel=5",
    "build:all": "nx run-many -t build --all --parallel=1",
    "clean": "rimraf dist",
    "clean:packages": "rimraf dist/packages",
    "commit": "node ./dist/packages/git-tools/bin/git.js commit --config=\"../src/commit/config.js\"",
    "readme-gen": "nx exec -- node ./dist/packages/git-tools/bin/git.js readme-gen --templates=\"./dist/packages/git-tools/readme/templates\"",
    "dep-graph": "nx dep-graph",
    "e2e": "nx e2e",
    "help": "nx help",
    "lint-biome": "nx exec -- node biome lint --apply --no-errors-on-unmatched --files-ignore-unknown=true",
    "lint-biome:check": "biome check --apply-unsafe --no-errors-on-unmatched --files-ignore-unknown=true",
    "format-biome": "nx exec -- node biome format --write  --no-errors-on-unmatched --files-ignore-unknown=true",
    "lint-filenames": "nx exec -- node ls-lint --config=\"./dist/packages/linting-tools/ls-lint/ls-lint.yml\"",
    "lint-toml": "nx exec -- node taplo format --check --config=\"./dist/packages/linting-tools/taplo/.taplo.toml\"",
    "format-toml": "nx exec -- node taplo format --config=\"./dist/packages/linting-tools/taplo/.taplo.toml\"",
    "lint": "nx exec -- node  ./dist/packages/linting-tools/bin/lint.js all --skip-cspell --skip-deps-version --skip-alex",
    "local-registry": "nx local-registry @storm-software/storm-ops",
    "nx": "nx",
    "release": "nx exec -- node  ./dist/packages/git-tools/bin/git.js release",
    "start": "nx serve",
    "test": "nx test"
  },
  "dependencies": {
    "@angular-devkit/architect": "^0.1702.0",
    "@angular-devkit/core": "^17.2.0",
    "@angular-devkit/schematics": "^17.2.0",
    "@biomejs/biome": "1.5.3",
    "@ls-lint/ls-lint": "^2.2.3",
    "@nx/devkit": "18.0.4",
    "@nx/esbuild": "18.0.4",
    "@nx/eslint": "18.0.4",
    "@nx/eslint-plugin": "18.0.4",
    "@nx/jest": "18.0.4",
    "@nx/js": "18.0.4",
    "@nx/plugin": "18.0.4",
    "@nx/workspace": "18.0.4",
    "@swc-node/register": "^1.8.0",
    "@swc/cli": "0.3.2",
    "@swc/core": "1.4.2",
    "@swc/helpers": "0.5.3",
    "@swc/wasm": "^1.4.2",
    "@taplo/cli": "^0.7.0",
    "@tsconfig/recommended": "1.0.3",
    "@types/jest": "29.5.11",
    "@types/node": "20.11.20",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "conventional-changelog-conventionalcommits": "^7.0.2",
    "esbuild": "0.19.12",
    "eslint": "8.56.0",
    "eslint-config-prettier": "^9.0.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-environment-node": "29.7.0",
    "lefthook": "1.6.1",
    "nx": "18.0.4",
    "prettier": "3.2.5",
    "rimraf": "5.0.5",
    "ts-jest": "29.1.2",
    "ts-node": "10.9.2",
    "tslib": "2.6.2",
    "typescript": "5.3.3",
    "verdaccio": "5.29.0"
  },
  "packageManager": "pnpm@8.14.3",
  "engines": {
    "node": ">=20.11.0",
    "pnpm": ">=8.10.2"
  },
  "namespace": "@storm-software",
  "nx": {
    "includedScripts": [
      "lint",
      "lint-biome",
      "lint-filenames",
      "lint-toml",
      "format",
      "format-biome",
      "format-readme",
      "format-toml",
      "release"
    ]
  },
  "pnpm": {
    "overrides": {
      "axios": ">=1.6.0",
      "got": ">=12.1.0",
      "chalk": "4.1.2",
      "tough-cookie": ">=4.1.3",
      "request": ">=2.88.2",
      "load-plugin": "3.0.0",
      "@cypress/request": ">=3.0.0",
      "create-storm-workspace": "workspace:*",
      "@storm-software/workspace-tools": "workspace:*",
      "@storm-software/linting-tools": "workspace:*",
      "@storm-software/git-tools": "workspace:*",
      "@storm-software/testing-tools": "workspace:*",
      "@storm-software/config-tools": "workspace:*",
      "@storm-software/build-tools": "workspace:*"
    },
    "patchedDependencies": {
      "tsup@8.0.0": "patches/tsup@8.0.0.patch",
      "cosmiconfig@9.0.0": "patches/cosmiconfig@9.0.0.patch",
      "@microsoft/api-extractor@7.40.1": "patches/@microsoft__api-extractor@7.40.1.patch"
    }
  },
  "triggerEmptyDevReleaseByIncrementingThisNumber": 0,
  "devDependencies": {
    "@monodon/rust": "^1.4.0"
  }
}