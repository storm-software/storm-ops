{
  "name": "@storm-software/storm-ops",
  "version": "0.1.24",
  "private": true,
  "description": "âš¡The Storm-Ops monorepo contains utility applications, tools, and various libraries to create modern and scalable web applications.",
  "keywords": [
    "storm",
    "storm-ops",
    "stormstack",
    "storm-stack",
    "acidic",
    "acidic-lang",
    "acidic-model",
    "impact",
    "nextjs",
    "prisma",
    "zenstack",
    "hasura",
    "strapi",
    "graphql",
    "sullivanpj",
    "open-system",
    "monorepo"
  ],
  "homepage": "https://stormsoftware.org",
  "bugs": {
    "url": "https://github.com/storm-software/storm-ops/issues",
    "email": "support@stormsoftware.org"
  },
  "repository": {
    "type": "github",
    "url": "https://github.com/storm-software/storm-ops.git"
  },
  "license": "Apache License 2.0",
  "author": {
    "name": "Storm Software",
    "email": "contact@stormsoftware.org",
    "url": "https://stormsoftware.org"
  },
  "scripts": {
    "adr": "pnpm log4brains adr new",
    "adr-preview": "pnpm log4brains preview",
    "api-extractor": "nx g @storm-software/workspace-tools:api-extractor --outputPath 'docs/api-reference' --clean --no-interactive",
    "prebuild": "pnpm clean",
    "build": "nx affected -t build --parallel=5",
    "build:all": "nx run-many -t build --all --parallel=5",
    "clean": "rimraf dist",
    "clean:packages": "rimraf dist/packages",
    "commit": "node ./dist/packages/git-tools/bin/git.js commit --config=\"../src/commit/config.js\"",
    "dep-graph": "nx dep-graph",
    "e2e": "nx e2e",
    "biome:check": "biome check --apply-unsafe --no-errors-on-unmatched --files-ignore-unknown=true",
    "biome:lint": "npx biome lint --apply --no-errors-on-unmatched --files-ignore-unknown=true",
    "biome:format": "npx biome format --write  --no-errors-on-unmatched --files-ignore-unknown=true",
    "help": "nx help",
    "preinstall": "node ./dist/packages/git-tools/bin/pre-install.js || npx -y only-allow pnpm",
    "lint": "node ./dist/packages/linting-tools/bin/lint.js all --skip-cspell --alex-config=\"./dist/packages/linting-tools/alex/.alexrc\" --alex-config=\"dist/packages/linting-tools/alex/.alexignore\"",
    "local-registry": "nx local-registry @storm-software/storm-ops",
    "nx": "nx",
    "release": "node ./dist/packages/git-tools/bin/git.js release",
    "start": "nx serve",
    "test": "nx test"
  },
  "dependencies": {
    "@biomejs/biome": "1.5.1",
    "@nx/devkit": "17.2.8",
    "@nx/esbuild": "17.2.8",
    "@nx/jest": "17.2.8",
    "@nx/js": "17.2.8",
    "@nx/plugin": "17.2.8",
    "@nx/workspace": "17.2.8",
    "@swc-node/register": "1.6.8",
    "@swc/cli": "0.1.62",
    "@swc/core": "1.3.96",
    "@swc/helpers": "0.5.3",
    "@types/jest": "29.5.8",
    "@types/node": "20.9.0",
    "conventional-changelog-conventionalcommits": "^7.0.2",
    "esbuild": "0.19.5",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-environment-node": "29.7.0",
    "lefthook": "^1.5.6",
    "nx": "17.2.8",
    "rimraf": "^5.0.5",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "tslib": "2.6.2",
    "typescript": "5.2.2",
    "verdaccio": "5.27.0"
  },
  "packageManager": "pnpm@8.10.2",
  "engines": {
    "node": ">=18.17.1",
    "pnpm": ">=8.10.2"
  },
  "namespace": "@storm-software",
  "nx": {
    "includedScripts": [
      "lint",
      "format"
    ]
  },
  "pnpm": {
    "overrides": {
      "axios": ">=1.6.0",
      "got": ">=12.1.0",
      "tough-cookie": ">=4.1.3",
      "request": ">=2.88.2",
      "@cypress/request": ">=3.0.0",
      "create-storm-workspace": "workspace:*",
      "@storm-software/workspace-tools": "workspace:*",
      "@storm-software/linting-tools": "workspace:*",
      "@storm-software/git-tools": "workspace:*",
      "@storm-software/testing-tools": "workspace:*",
      "@storm-software/design-tools": "workspace:*",
      "@storm-software/config-tools": "workspace:*"
    },
    "patchedDependencies": {
      "tsup@8.0.0": "patches/tsup@8.0.0.patch"
    }
  },
  "triggerEmptyDevReleaseByIncrementingThisNumber": 0
}
