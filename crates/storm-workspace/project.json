{
  "name": "storm-workspace",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "crates/storm-workspace/src",
  "targets": {
    "build": {
      "executor": "@monodon/rust:check",
      "outputs": [
        "{options.target-dir}"
      ],
      "options": {
        "target-dir": "dist/target/storm-workspace"
      }
    },
    "test": {
      "executor": "@monodon/rust:test",
      "outputs": [
        "{options.target-dir}"
      ],
      "options": {
        "target-dir": "dist/target/storm-workspace"
      },
      "configurations": {
        "production": {
          "release": true
        }
      }
    },
    "lint": {
      "executor": "@monodon/rust:lint",
      "outputs": [
        "{options.target-dir}"
      ],
      "options": {
        "target-dir": "dist/target/storm-workspace"
      }
    },
    "nx-release-publish": {
      "cache": false,
      "inputs": [
        "default",
        "^production"
      ],
      "dependsOn": [
        "^build"
      ],
      "executor": "@storm-software/workspace-tools:crates-publish",
      "options": {
        "packageRoot": "crates/storm-workspace"
      }
    }
  },
  "tags": []
}