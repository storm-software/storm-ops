{
  "id": "#",
  "title": "Npm Publish Executor",
  "description": "A type definition for a Npm Publish executor schema",
  "properties": {
    "packageRoot": {
      "title": "Package Root",
      "type": "string",
      "format": "path",
      "description": "The path to the package root",
      "id": "#packageRoot"
    },
    "registry": {
      "title": "Registry",
      "type": "string",
      "description": "The registry to publish to",
      "id": "#registry",
      "default": "https://registry.npmjs.org/"
    },
    "tag": {
      "title": "Tag",
      "type": "string",
      "description": "The tag to publish with",
      "id": "#tag",
      "default": "latest"
    },
    "otp": {
      "title": "One Time Password",
      "type": "number",
      "description": "The one time password",
      "id": "#otp"
    },
    "dryRun": {
      "title": "Dry Run",
      "type": "boolean",
      "description": "Perform a dry run",
      "id": "#dryRun",
      "default": false
    },
    "firstRelease": {
      "title": "First Release",
      "type": "boolean",
      "description": "Publish the first release",
      "id": "#firstRelease",
      "default": false
    }
  },
  "type": "object",
  "default": {
    "registry": "https://registry.npmjs.org/",
    "tag": "latest",
    "dryRun": false,
    "firstRelease": false
  }
}