{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "module": "ES2022",
    "lib": ["es2021"],
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "allowJs": true,
    "noImplicitOverride": true,
