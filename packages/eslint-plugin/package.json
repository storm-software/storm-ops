    "@nx/eslint": {
      "optional": true
    },
    "@nx/eslint-plugin": {
      "optional": true
    },
    "@storm-software/eslint": {
      "optional": false
    },
    "eslint": {
      "optional": false
    }
  },
  "dependencies": {
    "@babel/helper-validator-identifier": "7.24.7",
    "@eslint-community/eslint-utils": "4.4.0",
