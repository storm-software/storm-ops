    "ci-info": "4.0.0",
    "clean-regexp": "1.0.0",
    "core-js-compat": "3.37.1",
    "esquery": "1.5.0",
    "fs-extra": "11.2.0",
    "indent-string": "5.0.0",
    "is-builtin-module": "4.0.0",
    "jsesc": "3.0.2",
    "lodash-es": "4.17.21",
    "pluralize": "8.0.0",
    "read-pkg-up": "11.0.0",
    "regexp-tree": "0.1.27",
    "regjsparser": "0.10.0",
    "semver": "7.6.2",
    "strip-indent": "4.0.0",
    "tslib": "^2.6.3"
  },
  "devDependencies": {
    "@nx/eslint": "^20.2.2",
    "@nx/eslint-plugin": "^20.2.2",
