  "private": false,
  "main": "./cjs/index.cjs",
  "module": "./esm/index.mjs",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./index.d.ts",
        "default": "./esm/index.mjs"
      },
      "require": {
        "types": "./index.d.ts",
        "default": "./cjs/index.cjs"
      },
      "default": {
        "types": "./index.d.ts",
        "default": "./esm/index.mjs"
      }
    },
    "./configs": {
      "import": {
        "types": "./index.d.ts",
        "default": "./esm/configs.mjs"
      },
      "require": {
        "types": "./index.d.ts",
        "default": "./cjs/configs.cjs"
      },
      "default": {
        "types": "./index.d.ts",
        "default": "./esm/configs.mjs"
      }
    },
    "./types": {
      "import": {
        "types": "./index.d.ts",
        "default": "./esm/types.mjs"
      },
      "require": {
        "types": "./index.d.ts",
        "default": "./cjs/types.cjs"
      },
      "default": {
        "types": "./index.d.ts",
        "default": "./esm/types.mjs"
      }
    }
  },
  "types": "./index.d.ts",
  "keywords": [
    "front-end",
    "back-end",
    "cli",
    "frameworks",
    "storm",
    "storm-ops",
    "sullivanpj",
    "eslint",
