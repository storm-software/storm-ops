      "default": {
        "types": "./dist/plugins/index.d.ts",
        "default": "./dist/plugins/index.js"
      }
    },
    "./plugins/*": {
      "import": {
        "types": "./dist/plugins/*.d.mts",
        "default": "./dist/plugins/*.mjs"
      },
      "require": {
        "types": "./dist/plugins/*.d.ts",
