    "./utilities/*": {
      "import": {
        "types": "./dist/utilities/*.d.mts",
        "default": "./dist/utilities/*.mjs"
      },
      "require": {
        "types": "./dist/utilities/*.d.ts",
        "default": "./dist/utilities/*.js"
      },
      "default": {
        "types": "./dist/utilities/*.d.ts",
        "default": "./dist/utilities/*.js"
      }
    }
