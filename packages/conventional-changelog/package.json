{
  "name": "conventional-changelog-storm-software",
  "version": "0.0.1",
  "type": "module",
  "description": "This package contains the shared conventional-changelog configuration used in Storm Software projects.",
  "repository": {
    "type": "github",
    "url": "https://github.com/storm-software/storm-ops.git",
    "directory": "packages/conventional-changelog"
  },
  "homepage": "https://stormsoftware.com",
  "bugs": "https://github.com/storm-software/storm-ops/issues",
  "author": {
    "name": "Storm Software",
    "email": "contact@stormsoftware.com",
    "url": "https://stormsoftware.com"
  },
  "maintainers": [
    {
      "name": "Storm Software",
      "email": "contact@stormsoftware.com",
      "url": "https://stormsoftware.com"
    },
    {
      "name": "Pat Sullivan",
      "email": "admin@stormsoftware.com",
      "url": "https://patsullivan.org"
    }
  ],
  "contributors": [
    {
      "name": "Storm Software",
      "email": "contact@stormsoftware.com",
      "url": "https://stormsoftware.com"
    }
  ],
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/storm-software"
  },
  "license": "Apache-2.0",
  "private": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    "./package.json": "./package.json",
    "./index": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "default": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "default": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "types": "./dist/index.d.ts",
  "keywords": [
    "conventional-changelog",
    "commitlint",
    "changelog",
    "storm",
    "storm-ops",
    "storm-software"
  ],
  "dependencies": {
    "@storm-software/config": "workspace:*",
    "@storm-software/config-tools": "workspace:*",
    "@storm-software/git-tools": "workspace:*",
    "compare-func": "^2.0.0",
    "conventional-changelog-writer": "^8.2.0",
    "defu": "catalog:"
  },
  "devDependencies": {
    "@types/compare-func": "^1.3.3",
    "@types/node": "catalog:",
    "tsup": "catalog:",
    "typescript": "^5.9.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false
}
