      "import": {
        "types": "./dist/whatBump.d.ts",
        "default": "./dist/whatBump.js"
      },
      "require": {
        "types": "./dist/whatBump.d.cts",
        "default": "./dist/whatBump.cjs"
      },
      "default": {
        "types": "./dist/whatBump.d.ts",
        "default": "./dist/whatBump.js"
      }
    },
    "./writer": {
      "import": {
        "types": "./dist/writer.d.ts",
        "default": "./dist/writer.js"
      },
      "require": {
        "types": "./dist/writer.d.cts",
        "default": "./dist/writer.cjs"
      },
      "default": {
        "types": "./dist/writer.d.ts",
        "default": "./dist/writer.js"
      }
    },
    "./types": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/types/index.js"
      },
      "require": {
        "types": "./dist/types/index.d.cts",
        "default": "./dist/types/index.cjs"
      },
      "default": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/types/index.js"
      }
    },
    "./types/*": {
      "import": {
        "types": "./dist/types/*.d.ts",
        "default": "./dist/types/*.js"
      },
      "require": {
        "types": "./dist/types/*.d.cts",
        "default": "./dist/types/*.cjs"
      },
      "default": {
        "types": "./dist/types/*.d.ts",
        "default": "./dist/types/*.js"
      }
    },
    "./utilities": {
      "import": {
        "types": "./dist/utilities/index.d.ts",
        "default": "./dist/utilities/index.js"
      },
      "require": {
        "types": "./dist/utilities/index.d.cts",
        "default": "./dist/utilities/index.cjs"
      },
      "default": {
        "types": "./dist/utilities/index.d.ts",
        "default": "./dist/utilities/index.js"
      }
    },
    "./utilities/*": {
      "import": {
        "types": "./dist/utilities/*.d.ts",
        "default": "./dist/utilities/*.js"
      },
      "require": {
        "types": "./dist/utilities/*.d.cts",
        "default": "./dist/utilities/*.cjs"
      },
      "default": {
        "types": "./dist/utilities/*.d.ts",
        "default": "./dist/utilities/*.js"
      }
    },
    "./templates/*": "./dist/templates/*"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*",
    "templates/**/*"
  ],
  "keywords": [
    "conventional-changelog",
    "commitlint",
    "changelog",
    "storm",
    "storm-ops",
    "storm-software"
  ],
  "dependencies": {
    "@storm-software/config": "1.131.1",
    "@storm-software/config-tools": "1.185.1",
    "compare-func": "^2.0.0",
    "conventional-changelog-writer": "^8.2.0",
    "defu": "catalog:"
  },
  "devDependencies": {
    "@types/compare-func": "^1.3.3",
    "@types/node": "catalog:",
    "tsup": "catalog:",
    "typescript": "^5.9.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false
}
