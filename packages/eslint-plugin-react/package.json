  "repository": {
    "type": "github",
    "url": "https://github.com/storm-software/storm-ops",
    "directory": "packages/eslint-plugin-react"
  },
  "homepage": "https://stormsoftware.com",
  "bugs": "https://github.com/storm-software/storm-ops/issues",
  "author": {
    "name": "Storm Software",
    "email": "contact@stormsoftware.com",
    "url": "https://stormsoftware.com"
  },
  "license": "Apache-2.0",
  "private": false,
  "main": "./cjs/index.cjs",
  "module": "./esm/index.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./index.d.ts",
        "default": "./esm/index.js"
      },
      "require": {
        "types": "./index.d.ts",
        "default": "./cjs/index.cjs"
      },
      "default": {
        "types": "./index.d.ts",
        "default": "./esm/index.js"
      }
    },
    "./configs": {
      "import": {
        "types": "./index.d.ts",
        "default": "./esm/configs.js"
      },
      "require": {
        "types": "./index.d.ts",
        "default": "./cjs/configs.cjs"
      },
      "default": {
        "types": "./index.d.ts",
        "default": "./esm/configs.js"
      }
    },
    "./types": {
      "import": {
        "types": "./index.d.ts",
        "default": "./esm/types.js"
      },
      "require": {
        "types": "./index.d.ts",
        "default": "./cjs/types.cjs"
      },
      "default": {
        "types": "./index.d.ts",
        "default": "./esm/types.js"
      }
    }
  },
  "types": "./index.d.ts",
  "keywords": [
    "back-end",
    "cli",
    "eslint",
    "eslint-plugin",
    "eslintplugin",
    "frameworks",
    "front-end",
    "monorepo",
    "storm",
    "storm-ops",
    "sullivanpj"
  ],
  "peerDependencies": {
    "@nx/eslint": "catalog:",
    "@nx/eslint-plugin": "catalog:",
