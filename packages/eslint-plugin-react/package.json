  "peerDependenciesMeta": {
    "@nx/eslint": {
      "optional": true
    },
    "@nx/eslint-plugin": {
      "optional": true
    },
    "@storm-software/eslint": {
      "optional": false
    },
    "eslint": {
      "optional": false
    }
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "7.16.0",
    "@typescript-eslint/type-utils": "7.12.0",
    "@typescript-eslint/utils": "7.12.0",
    "fs-extra": "11.2.0",
    "tslib": "catalog:"
  },
  "devDependencies": {
    "@nx/eslint": "catalog:",
    "@nx/eslint-plugin": "catalog:",
    "@storm-software/eslint": "0.105.0",
    "@types/eslint": "^8.56.10",
    "@types/node": "catalog:",
    "eslint": "catalog:",
