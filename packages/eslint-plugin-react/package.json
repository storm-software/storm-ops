{
  "name": "eslint-plugin-storm-react",
  "version": "0.6.0",
  "type": "module",
  "description": "âš¡ The eslint-plugin package is an ESLint plugin that contains a collection of recommended ESLint rule configurations which you can extend from in your own ESLint configs, as well as an Nx-specific lint rule called enforce-module-boundaries.",
  "repository": {
    "type": "github",
    "url": "https://github.com/storm-software/storm-ops",
    "directory": "packages/eslint-plugin-react"
  },
  "homepage": "https://stormsoftware.com",
  "bugs": {
    "url": "https://github.com/storm-software/storm-ops/issues",
    "email": "support@stormsoftware.com"
  },
  "author": {
    "name": "Storm Software",
    "email": "contact@stormsoftware.com",
    "url": "https://stormsoftware.com"
  },
  "license": "Apache-2.0",
  "private": false,
  "main": "./cjs/index.cjs",
  "module": "./esm/index.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./index.d.ts",
        "default": "./esm/index.js"
      },
      "require": {
        "types": "./index.d.ts",
        "default": "./cjs/index.cjs"
      },
      "default": {
        "types": "./index.d.ts",
        "default": "./esm/index.js"
      }
    },
    "./configs": {
      "import": {
        "types": "./index.d.ts",
        "default": "./esm/configs.js"
      },
      "require": {
        "types": "./index.d.ts",
        "default": "./cjs/configs.cjs"
      },
      "default": {
        "types": "./index.d.ts",
        "default": "./esm/configs.js"
      }
    },
    "./types": {
      "import": {
        "types": "./index.d.ts",
        "default": "./esm/types.js"
      },
      "require": {
        "types": "./index.d.ts",
        "default": "./cjs/types.cjs"
      },
      "default": {
        "types": "./index.d.ts",
        "default": "./esm/types.js"
      }
    }
  },
  "types": "./index.d.ts",
  "keywords": [
    "front-end",
    "back-end",
    "cli",
    "frameworks",
    "storm",
    "storm-ops",
    "sullivanpj",
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "monorepo"
  ],
  "peerDependencies": {
    "@nx/eslint": ">=19.6.4",
    "@nx/eslint-plugin": ">=19.6.4",
    "@storm-software/eslint": "latest",
    "eslint": "9.5.0"
  },
  "peerDependenciesMeta": {
    "@nx/eslint": {
      "optional": true
    },
    "@nx/eslint-plugin": {
      "optional": true
    },
    "@storm-software/eslint": {
      "optional": false
    },
    "eslint": {
      "optional": false
    }
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "7.16.0",
    "@typescript-eslint/type-utils": "7.12.0",
    "@typescript-eslint/utils": "7.12.0",
    "fs-extra": "11.2.0",
    "tslib": "^2.6.3"
  },
  "devDependencies": {
    "@nx/eslint": ">=19.6.4",
    "@nx/eslint-plugin": ">=19.6.4",
    "@storm-software/eslint": "0.69.5",
    "@types/eslint": "^8.56.10",
    "eslint": "9.5.0",
    "eslint-doc-generator": "1.7.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
