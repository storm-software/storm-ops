{
  "organization": "storm-software",
  "owner": "@storm-software/development",
  "timezone": "America/New_York",
  "locale": "en-US",
  "logLevel": "all",
  "skipCache": false,
  "cacheDirectory": "node_modules/.cache/storm",
  "buildDirectory": "dist",
  "outputDirectory": "node_modules/.storm",
  "packageManager": "pnpm",
  "externalPackagePatterns": ["@storm-software/"],
  "registry": {
    "github": "https://npm.pkg.github.com",
    "npm": "https://registry.npmjs.org",
    "cargo": "https://crates.io",
    "cyclone": "https://registry.cyclone-ui.com/components",
    "container": "https://hub.docker.com"
  },
  "colors": {
    "light": {
      "background": "#fafafa",
      "foreground": "#1d1e22",
      "brand": "#1fb2a6",
      "alternate": "#db2777",
      "link": "#01B4F5",
      "help": "#5C4EE5",
      "success": "#087f5b",
      "info": "#0550ae",
      "warning": "#e3b341",
      "error": "#a40e26",
      "positive": "#22c55e",
      "negative": "#dc2626"
    },
    "dark": {
      "background": "#1d1e22",
      "foreground": "#cbd5e1",
      "brand": "#2dd4bf",
      "alternate": "#db2777",
      "link": "#01B4F5",
      "help": "#818cf8",
      "success": "#10b981",
      "info": "#58a6ff",
      "warning": "#f3d371",
      "error": "#d1242f",
      "positive": "#22c55e",
      "negative": "#dc2626"
    }
  },
  "extensions": {
    "telemetry": {
      "fileName": "storm",
      "fileExtension": "log",
      "path": "tmp/storm/logs",
      "stacktrace": true
    }
  }
}
