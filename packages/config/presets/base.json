{
  "organization": "storm-software",
  "ci": true,
  "owner": "@storm-software/development",
  "timezone": "America/New_York",
  "locale": "en-US",
  "logLevel": "all",
  "skipCache": false,
  "cacheDirectory": "node_modules/.cache/storm",
  "buildDirectory": "dist",
  "outputDirectory": "node_modules/.storm",
  "packageManager": "npm",
  "externalPackagePatterns": ["@storm-software/"],
  "colors": {
    "light": {
      "background": "#f4f4f5",
      "foreground": "#1d232a",
      "brand": "#1fb2a6",
      "brand2": "#8250df",
      "success": "#087f5b",
      "info": "#0550ae",
      "warning": "#e3b341",
      "error": "#a40e26"
    },
    "dark": {
      "background": "#22272E",
      "foreground": "#fcfcf5",
      "brand": "#1fb2a6",
      "brand2": "#8256D0",
      "success": "#087f5b",
      "info": "#316DCA",
      "warning": "#F3D371",
      "error": "#d1242f"
    }
  },
  "extensions": {
    "telemetry": {
      "fileName": "storm",
      "fileExtension": "log",
      "path": "tmp/storm/logs",
      "stacktrace": true
    }
  }
}
