{
  "organization": "storm-software",
  "owner": "@storm-software/development",
  "timezone": "America/New_York",
  "locale": "en-US",
  "logLevel": "all",
  "skipCache": false,
  "cacheDirectory": "node_modules/.cache/storm",
  "buildDirectory": "dist",
  "outputDirectory": "node_modules/.storm",
  "packageManager": "pnpm",
  "externalPackagePatterns": ["@storm-software/"],
  "registry": {
    "github": "https://npm.pkg.github.com",
    "npm": "https://registry.npmjs.org",
    "cargo": "https://crates.io",
    "cyclone": "https://registry.cyclone-ui.com/components",
    "container": "https://hub.docker.com"
  },
  "colors": {
    "light": {
      "background": "#f4f4f5",
      "foreground": "#1d232a",
      "brand": "#3fc1b0",
      "brand2": "#7434DB",
      "brand3": "#3D90CE",
      "help": "#8250df",
      "success": "#087f5b",
      "info": "#0550ae",
      "warning": "#e3b341",
      "error": "#a40e26"
    },
    "dark": {
      "background": "#212830",
      "foreground": "#fcfcf5",
      "brand": "#3fc1b0",
      "brand2": "#7434DB",
      "brand3": "#3D90CE",
      "help": "#8256D0",
      "success": "#087f5b",
      "info": "#316DCA",
      "warning": "#F3D371",
      "error": "#d1242f"
    }
  },
  "extensions": {
    "telemetry": {
      "fileName": "storm",
      "fileExtension": "log",
      "path": "tmp/storm/logs",
      "stacktrace": true
    }
  }
}
