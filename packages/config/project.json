{
  "name": "config",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "packages/config/src",
  "targets": {
    "build": {
      "executor": "@nx/esbuild:esbuild",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "main": "packages/config/src/index.ts",
        "outputPath": "dist/packages/config",
        "tsConfig": "packages/config/tsconfig.json",
        "project": "packages/config/package.json",
        "defaultConfiguration": "production",
        "platform": "neutral",
        "deleteOutputPath": true,
        "bundle": true,
        "thirdParty": true,
        "skipTypeCheck": false,
        "metafile": true,
        "minify": true,
        "format": [
          "cjs"
        ],
        "esbuildOptions": {
          "outExtension": {
            ".js": ".js"
          }
        },
        "assets": [
          {
            "input": "./packages/config",
            "glob": "*.md",
            "output": "."
          },
          {
            "input": "",
            "glob": "LICENSE",
            "output": "."
          },
          {
            "input": "./packages/config",
            "glob": "declarations.d.ts",
            "output": "."
          },
          {
            "input": "./packages/config",
            "glob": "presets/*.json",
            "output": "."
          },
          {
            "input": "./packages/config",
            "glob": "schema/storm.schema.json",
            "output": "."
          }
        ],
        "configurations": {
          "production": {
            "debug": false,
            "verbose": false
          },
          "development": {
            "debug": true,
            "verbose": true
          }
        }
      }
    },
    "lint": {},
    "test": {}
  }
}