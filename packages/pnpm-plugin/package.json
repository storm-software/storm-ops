{
  "name": "pnpm-plugin-storm-software",
  "version": "0.0.1",
  "type": "module",
  "description": "A [pnpm](https://pnpm.io/) plugin to enhance pnpm functionality for Storm Software projects.",
  "repository": {
    "type": "github",
    "url": "https://github.com/storm-software/storm-ops",
    "directory": "packages/pnpm-plugin"
  },
  "private": false,
  "main": "./pnpmfile.cjs",
  "exports": {
    "./package.json": "./package.json",
    ".": "./pnpmfile.cjs",
    "./pnpmfile.cjs": "./pnpmfile.cjs",
    "./allow.json": "./allow.json"
  },
  "files": ["./pnpmfile.cjs", "./allow.json"],
  "dependencies": {
    "@pnpm/plugin-better-defaults": "catalog:",
    "@pnpm/plugin-esm-node-path": "catalog:",
    "defu": "catalog:"
  },
  "devDependencies": {
    "@pnpm/types": "catalog:",
    "@types/node": "catalog:",
    "tsup": "catalog:"
  },
  "publishConfig": {
    "access": "public"
  }
}
