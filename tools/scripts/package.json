{
  "name": "@storm-software/tools-scripts",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./*": "./dist/*"
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "echo Skipping `tools-scripts` build",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "consola": "3.3.3",
    "esbuild": "catalog:",
    "pkg-types": "catalog:",
    "read-yaml-file": "2.1.0",
    "tslib": "catalog:"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "copyfiles": "catalog:",
    "rimraf": "catalog:",
    "tsup": "catalog:"
  }
}
